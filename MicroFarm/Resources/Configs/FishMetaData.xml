<?xml version="1.0" encoding="utf-8"?>
<ArrayOfFishMetaData xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<FishMetaData>
		<!--名称-->
		<Name>标准鱼</Name>
		<!--类型Id-->
		<Category>0</Category>
		<!--一个标准周期为1分钟-->
		<!--出生周期-->
		<BirthAge>1</BirthAge>
		<!--成年周期，成年有几率触发生育-->
		<AdultAge>180</AdultAge>
		<!--最大周期，周期达到最大有概率死亡-->
		<MaxAge>360</MaxAge>
		<!--标准移动速度,最快为20，值越大，越慢-->
		<DefaultSpeed>20</DefaultSpeed>
		<!--最大尺寸-->
		<MaxSize>50</MaxSize>
		<!--图片资源-->
		<Source>fish</Source>
		<!--正常死亡率-->
		<NormalDeathRate>0.05</NormalDeathRate>
		<!--最大周期死亡率-->
		<MaxAgeDeathRate>0.1</MaxAgeDeathRate>
	</FishMetaData>
</ArrayOfFishMetaData>