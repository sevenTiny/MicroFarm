	<FishMetaData>
		<!--名称-->
		<Name>标准鱼（雄）</Name>
		<!--类型Id（唯一）-->
		<Category>0</Category>
		<!--种，同种类的鱼‘种’值一样，在生育时，在同种之间随机生成个体-->
		<Species>0</Species>
		<!--一个标准周期为1分钟-->
		<!--出生周期-->
		<BirthAge>1</BirthAge>
		<!--成年周期，成年有几率触发生育-->
		<AdultAge>180</AdultAge>
		<!--最大周期，周期达到最大有概率死亡-->
		<MaxAge>360</MaxAge>
		<!--标准移动速度,最快为20，值越大，越慢-->
		<DefaultSpeed>20</DefaultSpeed>
		<!--最大尺寸-->
		<MaxSize>50</MaxSize>
		<!--图片资源-->
		<Source>fish_0.png</Source>
		<!--正常死亡率-->
		<NormalDeathRate>0.0005</NormalDeathRate>
		<!--最大周期死亡率-->
		<MaxAgeDeathRate>0.0005</MaxAgeDeathRate>
		<!--生殖率-->
		<ReproductionRate>0</ReproductionRate>
		<!--生殖关联种类，如果存在关联，则水族中必须同时存在另一种成年鱼，才会触发生殖。如果不需要关联(自我繁殖)，则可以填自己-->
		<ReproductionRelationCategory>-1</ReproductionRelationCategory>
		<!--移动时是否有角度变换（水母这类的不需要角度变化）-->
		<UseAngle>1</UseAngle>
	</FishMetaData>
